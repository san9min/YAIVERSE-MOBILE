{
  "expo": {
    "name": "YAIVERSE",
    "slug": "YAIVERSE Mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#1B192C"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.san9min.YAIVERSE"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#1B192C"
      },
      "package": "com.san9min.YAIVERSE"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "사진을 바로 촬영해 아바타를 만들 수 있습니다."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "YAIverse가 회원님의 사진에 접근하도록 허용해주세요.",
          "savePhotosPermission": "YAIverse가 아바타를 갤러리에 저장하도록 허용해주세요.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ]
  }
}
